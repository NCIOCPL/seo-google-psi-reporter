#!/usr/bin/env node
'use strict';

const { Command } = require('commander');
const { FetchInsights } = require('../cmds');

const main = async () => {
	const program = new Command();
	program.version('2.0.0');
	program.addCommand(FetchInsights());

	await program.parseAsync(process.argv);

	if (program.args.length < 1 ) {
		console.log('No command specified. See \'seo-google-psi-reporter --help\':');
		program.outputHelp();
		process.exit(1);
	}
}

main();